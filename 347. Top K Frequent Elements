class Solution {
public:
    int mapmax(map<int,int> m){
        int temp = INT_MIN,tempID;
        for(auto& i:m){
            if (i.second>temp){
                temp = i.second;
                tempID = i.first;
            }
            
        }
    return tempID;
    }
    
    
    vector<int> topKFrequent(vector<int>& nums, int k) {
        map<int,int> m;
        for (int i:nums){
            m[i]+=1;
        }
        vector<int> ans;
        for (int i=0; i<k;++i){
            ans.push_back(mapmax(m));
            m.erase(mapmax(m));
        }
        return ans;
    }
};
